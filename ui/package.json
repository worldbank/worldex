    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-rewrite-all": "^1.0.1",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "eject": "react-scripts eject",
    "lint": "eslint './**/*.{ts,tsx}' --ignore-path .gitignore",
    "format": "prettier --write . --ignore-path .gitignore",
    "hygen": "hygen",
    "updateSupportedBrowsers": "echo \"var browserRegex = $(npx browserslist-useragent-regexp --allowHigherVersions);\" > public/supportedBrowsers.js",
    "prepare": "cd .. && husky install"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es6": true,
      "browser": true
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-console": 1,
      "no-debugger": 1,
      "prettier/prettier": 1
    },
    "extends": [
      "react-app",
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      "defaults",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "@deck.gl/core": "^8.9.19",
    "@deck.gl/layers": "^8.9.19",
    "@deck.gl/extensions": "^8.9.19",
    "@deck.gl/geo-layers": "^8.9.19",
    "@luma.gl/core": "^8.5.20"
  },
  "resolutions": {
    "@deck.gl/core": "^8.9.19",
    "@deck.gl/layers": "^8.9.19",
    "@deck.gl/extensions": "^8.9.19",
    "@deck.gl/geo-layers": "^8.9.19",
    "@luma.gl/core": "^8.5.20"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxSingleQuote": true,
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,ts,tsx,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3"
  }
}
